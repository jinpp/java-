package Test;

public class Bubblesort {

	public static void main(String[] args) {
		// TODO 自动生成的方法存根
		int a[] = { 95, 45, 15, 78, 84, 51, 24, 12 };
		for (int i = a.length - 1; i > 0; i--) {
			for (int j = 0; j < i; j++) {
				int temp = 0;
				if (a[j + 1] < a[j]) {
					temp = a[j + 1];
					a[j + 1] = a[j];
					a[j] = temp;
				}
			}
		}
	System.out.println("冒泡排序数组："+a[0]);	
	}

}
